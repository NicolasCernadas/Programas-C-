//
#include <stdio.h>
void printancia(){
    printf("esto es la funcion printancia \n");
}
void sumaEntera(int a, int b){
    int c = a+b;
    printf("%i",c);
}
int sumaEnteraD(int a, int b){
    int c = a+b;
    return c;
}
void main() {
    printf("Hello, World!\n");
    printancia();
    sumaEntera(3,6);
    int c = sumaEnteraD(3,6);
    //return 0;
}
//
**FUNCIONES LOCALES: terminan dentro de sus parametros ({})**
Memoria global: aquella que no esta creada en ninguna funcion, es igual para todo el codigo.

void: No devuelve valores
int: Si devuelve valores

1°)Creacion de funciones VOID: (que no devuelven valores)

void "nombre de la funcion"('lo que vaya a recibir la funcion'){
  (lo que quieras que haga la funcion "nombre de la funcion");
}

ejemplo:
void sumaEntera(int a, int b){
  int c= a + b;
printf("&i",c);
}
Para llamar a la funcion creada en cualquier parte del codigo:
"nombre de la funcion";

2°)Creacion de funciones INT: (devuelve valores)

ejemplo:
int sumaEntera(int a, int b){
  int c= a + b;
  return c;
}
Para llamar a la funcion creada en cualquier parte del codigo:
int c= sumaEntera(a,b);

##EJEMPLO FUNCIONES##

#include <stdio.h>

void multiplo (int x, int y);
int main(){
    int n1, n2;
    do{
        printf("Ingresa 2 numeros: ");
        scanf("%d%d",&n1,&n2);
        if (n1>n2){
            printf("El segundo es menor al primero, ingrese primero un numero y despues uno mayor\n");
        }
    } while (n1>n2);
    multiplo(n1, n2);
return 0;}

void multiplo (int x, int y){
    if ((y%x)==0){
        printf("%d es multiplo de %d",x,y);
    }
    else {
        printf("%d NO es multiplo de %d",x,y);
    }
}

#include <stdio.h>

void cuadrados_anteriores(int x);

int main(){
    int n1;
    printf("Ingrese el numero maximo a sumar los cuadrados: ");
    scanf("%d",&n1);
    cuadrados_anteriores(n1);
return 0;}

void cuadrados_anteriores(int x){
int cuadrado, suma=0;
    for (int i=1;i<=x;i++){
        printf("N = %d\n",i);
        cuadrado = i*i;
        printf("N * N = %d\n",cuadrado);
        suma += cuadrado;
        printf("La suma va: %d\n\n",suma);
    }
}
