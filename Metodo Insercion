//Ordenamiento por incersion
#include<stdio.h>

int main(){
    int numeros [] = {3,5,4,1,2};
    int posicion, aux;

    for (int i=0; i<5; i++){
        posicion = i;
        aux = numeros[i];
        while ((posicion>0) && (numeros[posicion - 1]>aux)){
            numeros[posicion] = numeros [posicion-1];
            posicion--;
        }
        numeros[posicion] = aux;
    }
    for (int o=0;o<5;o++){
        printf("%d, ", numeros[o]);
    }


return 0;
}
