#include<stdio.h>

int main(){
    int cadena[]={4,1,2,3,5};
    int i,t,f,aux,longitud=0;

    for (int a=0;cadena[a]!='\0';a++){
        if (cadena[a]!='\0'){
            longitud += 1;
        }else {}
    }

    printf("Longitud del array: %d\n",longitud);
    for(i=0;i<(longitud-1);i++){
        for (t = (i + 1); t<longitud;t++){
            if (cadena[i] > cadena[t]){
                aux = cadena[i];
                cadena[i] = cadena[t];
                cadena[t] = aux;
            }
        }
    }
    
    for (f=0;f<longitud;f++){
        printf("%d", cadena[f]);
    }

return 0;
}
